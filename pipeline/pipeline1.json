{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "XmlSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false,
							"prefix": "sample"
						},
						"formatSettings": {
							"type": "XmlReadSettings",
							"validationMode": "none",
							"namespaces": true
						}
					},
					"sink": {
						"type": "AzureTableSink",
						"azureTableInsertType": "replace",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['catalog']['book']['title']"
								},
								"sink": {
									"name": "n1",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['catalog']['book']['genre']"
								},
								"sink": {
									"name": "n2",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['catalog']['book']['price']"
								},
								"sink": {
									"name": "n3",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "source_SG",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}